{
    "name": "liip/translation-bundle",
    "type": "symfony-bundle",
    "description": "provide various tools to ease translation management",
    "keywords": ["translation", "i18n"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Liip AG",
            "homepage": "http://www.liip.ch/"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/liip/LiipTranslationBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.0"
    },
    "require-dev": {
        "twig/twig": ">=1.0,<2.0-dev",
        "symfony/symfony": "~2.0",
        "liip/rmt": "dev-master"
    },
    "suggest": {
        "twig/twig": ">=1.0,<2.0-dev"
    },
    "autoload": {
        "psr-0": { "Liip\\TranslationBundle": "" }
    },
    "target-dir": "Liip/TranslationBundle"
}
